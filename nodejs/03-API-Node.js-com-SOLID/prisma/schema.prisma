// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

//tabela no prisma é chamado de model o nome da tabela é no singular.
model User {
  id    String @id @default(uuid())
  name  String
  email String @unique

  //para mudar o nome da tabela de singular para plural use @@map

  @@map("users")
}
